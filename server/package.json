  "scripts": {
    "start": "node --optimize_for_size --max-old-space-size=10024 index.mjs",
    "ssr": "set BABEL_CACHE_PATH=./node_modules/babel-cache.json && babel-node --presets env index.mjs",
    "build": "babel ./front/src --out-dir ./dist --extensions '.js,.jsx,.ts,.tsx'",
    "dev": "nodemon index.mjs",
    "test": "node -r @babel/register index.js",
    "test2": "yarn build && node --loader ts-node/esm ./front/src/ssr.mjs",
    "test3": "yarn babel-node ./front/src/ssr.mjs --extensions '.ts,.tsx'",
    "test4": "NODE_OPTIONS='--experimental-loader ./alias-loader.mjs' node index.mjs",
    "test5": "NODE_OPTIONS='--experimental-loader ./alias-loader.mjs' node --no-warnings index.mjs"
  },
  "main": "index.mjs",
  "devDependencies": {
    "@babel/cli": "^7.19.3",
